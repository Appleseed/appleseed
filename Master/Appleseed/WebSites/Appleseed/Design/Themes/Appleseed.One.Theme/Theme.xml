  <Parts>
    <ThemePart Name="ModuleLayout">
      <HTML>
        <![CDATA[	
	<div class="clear"> </div>
	<div class="ModuleContent">
		<ul class="Control_Panel_Table">		
			<li><a href="#"><img src="icon/brick.png" alt="Change module" valign="bottom" ></a>
				<ul>
					{ControlPanel}
				</ul>
			</li>
		</ul>	
		<h1>{Title}</h1>
		<hr class="divider"/>
		<div class="module_Body">
			{Body}
		</div>
        </div>
      ]]>
      </HTML>
    </ThemePart>
    <ThemePart Name="DefaultBodyBgColor">
      <HTML>#fff</HTML>
    </ThemePart>
    <ThemePart Name="DefaultTitleBgColor">
      <HTML>#ae2929</HTML>
    </ThemePart>
    <ThemePart Name="TitleBeforeButton">
      <HTML><![CDATA[<li>]]></HTML>
    </ThemePart>
    <ThemePart Name="TitleAfterButton">
      <HTML><![CDATA[</li>]]></HTML>
    </ThemePart>
  </Parts>
